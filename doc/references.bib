@article{Rougeux2019,
   author = {Rougeux, C. and Gagnaire, P.A. and Bernatchez, L.},
   title = "{Model-based demographic inference of introgression history in European whitefish species pairs}",
   journal = {J. Evol. Biol.},
   volume = {32},
   pages = {806--817},
   year = {2019},
   doi = {10.1111/jeb.13482},
   note = {Utilitzen l'espectre de freqüències al·lèliques i el programa "dadi" per estudiar les històries demogràfiques dels peixos en quatre llacs: Zurich, Lucern, Langfjordvatn i Skrukkebukta. Les dades són de RNA-seq, de 6 individus de cada una de les dues espècies de cada llac. Ajusten quatre tipus de models a cada llac (per separat), i en tots ells els models de contacte secundari expliquen millor les dades. És a dir, cada parella d'espècies (bèntica i limnètica) sembla haver divergit molt abans d'arribar al llac, i haver intercanviat variaicó genètica en els últims mil·lennis. Els models inclouen estimacions de les porcions del genoma que no han patit tanta introgressió o que han vist la seua mida efectiva reduïda com a conseqüència de la selecció natural o de la selecció de fons. Però no van ajustar cap model a les dades de dos o més llacs junts. Sense incloure en el mateix model dades d'almenys dos llacs, la conclusió aparent de què l'especiació no ha estat simpàtrica depén de la qualitat de les estimacions dels temps de divergència i temps de contacte. No em fie del resultat.}
}
@article{Hakli2017,
   author = {Häkli, K. and Østbye, K. and Kahilainen, K.K. and Amundsen, P.A.},
   title = "{Diversifying selection drives parallel evolution of gill raker number and body size along the speciation continuum of European whitefish}",
   journal = {Ecology and Evolution},
   volume = {8},
   pages = {2617--2631},
   year = {2017},
   doi = {10.1002/ece3.3876},
   note = {El complex C. lavaretus colonità el nord de Fennoscàndia bastant després d'haver arribat als Alps, i ho va fer d'est a oest, passint primer per la conca de Pasvik (on està Langsfjordvatn), la de Tana i finalment la d'Alta (on està Suopatjavri). Com a conseqüència, hi ha un gradient de diversitat genètica, així com un gradient de diferenciació entre els tres ecomorfs observables d'est a oest. Inclou bones referències sobre la definició dels ecomorfs. La pregunta que es fan els autors és si la diferènciació fenotípica dels ecomorfs va ser realment promoguda per la selecció natural i és adaptativa o bé és resultat de la deriva genètica. Utilitzen uns models complexos que jo desconec que permeten relacionar la diversitat morfològica i la genètica i extraure conclusions. És super-interessant, i m'haig de posar al dia amb això. La conclusió sembla ser que, efectivament, la radiació fenotípica fou adaptativa, com cabia esperar. Cal donar una ullada al paquet Driftsel. Les dades genètiques utilitzades provenen de microsatèl·lits.}
}
@article{Fang2022,
   author = {Fang, B. and Momigliano, P. and Kahilainen, K.K. and Merilä, J.},
   title = {Allopatric origin of sympatric whitefish morphs with insights on the genetic basis of their reproductive isolation},
   journal = {Evolution},
   volume = {76},
   number = {8},
   pages = {1905-1913},
   doi = {10.1111/evo.14559},
   year = {2022},
   note = {En un llac concret, on els Coregonus sp es van introduir cap als anys 60, resulta que hi ha tres ecomorfs, i òbviament es demostra que provenen d'introducció simultània dels tres morfs. No és que hagen especiat allà en els últims 50 anys. Les dades genètiques provenen de 2b-RAD-seq, i els mètodes són bons. Valdrà la pena llegir-lo.}
}
